#include "/home/michael/.colors/gohucolor"

! #########################################
urxvt.termName: rxvt-unicode
urxvt*perl-lib:			/usr/lib/urxvt/perl
urxvt*perl-ext-common:		default,matcher,clipboard
urxvt*perl-matcher: 		1
urxvt*matcher-button: 		1
urxvt*urlLauncher:		/usr/bin/google-chrome
urxvt*cursorUnderline: 1
!urxvt*letterspace: -1
urxvt*fading: 			10
urxvt*fadeColor: 		#000000	
urxvt*cursorBlink:		true
urxvt*scrollBar:		false
urxvt*depth:		32

URxvt*font: xft:uushi:pixelsize=12
URxvt*boldFont: xft:uushi:pixelsize=12

!URxvt*font: xft:lemon:pixelsize=12
!URxvt*boldFont: xft:lemon:pixelsize=12

!URxvt*font: xft:limey:pixelsize=12
!URxvt*boldFont: xft:limey:pixelsize=12

Xft.dpi:	96
Xft.rgba:	rgb
Xft.antialias: 	true
Xft.hinting: 	true
Xft.autohint: 	false
Xft.hintstyle: 	hintslight
Xft.lcdfilter: 	lcddefault
