#include "/home/michael/.colors/fallac"

! -- Cursor Theme --
Xcursor.theme: Adwaita

! #########################################
urxvt.termName: rxvt-unicode
urxvt*perl-lib:			/usr/lib/urxvt/perl
urxvt*perl-ext-common:		default,matcher,clipboard,tabbed
urxvt*perl-matcher: 		1
urxvt*matcher-button: 		1
urxvt*urlLauncher:		/usr/bin/google-chrome
urxvt*cursorUnderline: 1
!urxvt*letterspace: -1
urxvt*fading: 			10
urxvt*fadeColor: 		#000000	
urxvt*cursorBlink:		true
urxvt*scrollBar:		false
urxvt*depth:		32

urxvt*font: xft:SourceCodePro-Regular:pixelsize=12
!urxvt*font: -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*boldFont: -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*italicFont: -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*

!urxvt*font: -*-SourceCodePro-Regular-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*boldFont: -*-SourceCodePro-Regular-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*italicFont: -*-SourceCodePro-Regular-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-SourceCodePro-Regular-*-*-*-*-*-*-*-*-*-*-*-*

!urxvt*font: -*-limey-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*boldFont: -*-limey-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*italicFont: -*-limey-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-limey-*-*-*-*-*-*-*-*-*-*-*-*

!Xft.dpi:	72
Xft.dpi:	96
Xft.rgba:	rgb
Xft.antialias: 	true
Xft.hinting: 	true
Xft.autohint: 	false
Xft.hintstyle: 	hintslight
Xft.lcdfilter: 	lcddefault
